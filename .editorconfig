# Editor configuration, see https://editorconfig.org
root = true

[*]
charset = utf-8
indent_style = space
indent_size = 4
insert_final_newline = true
trim_trailing_whitespace = true

[*.json]
indent_size = 4
indent_style = tab
insert_final_newline = true

[*.scss]
indent_size = 4
insert_final_newline = false
